IDD_LOADER DIALOG DISCARDABLE  105, 80, 259, 218
STYLE WS_MINIMIZEBOX | WS_CAPTION | WS_SYSMENU
CAPTION "Hack by eVOL"
FONT 10, "宋体"
BEGIN
    CONTROL         "-=[eVOL]=- > All Rights Served.",
                    IDC_STATIC,"Static",SS_LEFTNOWORDWRAP | WS_GROUP,8,203,
                    191,8
    LTEXT           "Credit: Liu Zewei.",
                    IDC_STATIC,8,190,186,9
