            VALUE "InternalName", "Hack by eVOL\0"
            VALUE "LegalCopyright", "-=[eVOL]=- > All Rights Served.\0"
            VALUE "LegalTrademarks", "\0"
            VALUE "OriginalFilename", "Loader.exe\0"
            VALUE "PrivateBuild", "\0"
            VALUE "ProductName", "Hack by eVOL\0"
            VALUE "ProductVersion", "0, 1, 0, 1\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
